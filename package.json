{
	"name": "transformalize",
	"displayName": "Transformalize",
	"description": "Runs Transformalize arrangements.",
	"version": "0.0.6",
	"publisher": "DaleNewman",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tfl.run",
		"onCommand:tfl.schema",
		"onCommand:tfl.init",
		"onCommand:tfl.schedule"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Transformalize",
			"properties": {
				"tfl.path": {
					"type": "string",
					"default": "",
					"description": "The path to tfl.exe"
				}
			}
		},
		"commands": [
			{
				"command": "tfl.run",
				"title": "TFL: run"
			},
			{
				"command": "tfl.schema",
				"title": "TFL: schema"
			},
			{
				"command": "tfl.init",
				"title": "TFL: init"
			},
			{
				"command": "tfl.schedule",
				"title": "TFL: schedule"
			}
		],
		"snippets": [
			{
				"language": "xml",
				"path": "./snippets/xml.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dalenewman/Transformalize.VisualStudioCode.git"
	},
	"homepage": "https://github.com/dalenewman/Transformalize.VisualStudioCode/blob/master/README.md"
}
