{
	"name": "transformalize",
	"displayName": "Transformalize",
	"description": "Runs Transformalize arrangements.",
	"version": "0.0.8",
	"publisher": "DaleNewman",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tfl.run",
		"onCommand:tfl.schema",
		"onCommand:tfl.init",
		"onCommand:tfl.schedule"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Transformalize",
			"properties": {
				"tfl.path": {
					"type": "string",
					"default": "",
					"description": "The path to tfl.exe"
				}
			}
		},
		"commands": [
			{
				"command": "tfl.run",
				"title": "TFL: run"
			},
			{
				"command": "tfl.schema",
				"title": "TFL: schema"
			},
			{
				"command": "tfl.init",
				"title": "TFL: init"
			},
			{
				"command": "tfl.schedule",
				"title": "TFL: schedule"
			}
		],
		"snippets": [
			{
				"language": "xml",
				"path": "./snippets/xml.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dalenewman/Transformalize.VisualStudioCode.git"
	},
	"homepage": "https://github.com/dalenewman/Transformalize.VisualStudioCode/blob/master/README.md"
}
